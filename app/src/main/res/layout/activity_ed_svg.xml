<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:gravity="center"
	android:background="#FFFFFFFF"
	android:padding="1dp">

	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="match_parent"
		android:layout_height="wrap_content">

		<TextView
			android:clickable="true"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="."
			android:gravity="center"
			android:textColor="#FF000000"
			android:textSize="16sp"
			android:padding="10dp"
			android:background="@drawable/button_x_fff"
			android:layout_margin="1dp"
			android:layout_weight="0.5"
			android:id="@+id/def1"/>

		<TextView
			android:clickable="true"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text=".."
			android:gravity="center"
			android:textColor="#FF000000"
			android:textSize="16sp"
			android:padding="10dp"
			android:background="@drawable/button_x_fff"
			android:layout_margin="1dp"
			android:layout_weight="0.5"
			android:id="@+id/def2"/>

		<TextView
			android:clickable="true"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="..."
			android:gravity="center"
			android:textColor="#FF000000"
			android:textSize="16sp"
			android:padding="10dp"
			android:background="@drawable/button_x_fff"
			android:layout_margin="1dp"
			android:layout_weight="0.5"
			android:id="@+id/def3"/>

		<TextView
			android:clickable="true"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="○"
			android:gravity="center"
			android:textColor="#FF000000"
			android:textSize="16sp"
			android:padding="10dp"
			android:background="@drawable/button_x_fff"
			android:layout_margin="1dp"
			android:layout_weight="0.5"
			android:id="@+id/my"/>

	</LinearLayout>

	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="match_parent"
		android:layout_height="wrap_content">

		<RelativeLayout
			android:layout_width="50dp"
			android:layout_height="50dp"
			android:background="@drawable/shape_x_fff"
			android:layout_margin="1dp"
			android:id="@+id/imgprt">

			<TextView
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:padding="1dp"
				android:gravity="center"
				android:id="@+id/imgt"
				android:layout_margin="1dp"
				android:textColor="#FF000000"/>

			<TextView
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:textSize="10sp"
				android:id="@+id/imgmsg"
				android:layout_marginTop="2dp"
				android:layout_marginLeft="2dp"
				android:textColor="#FF999999"
				android:textStyle="bold"/>

		</RelativeLayout>

		<GridView
			android:cacheColorHint="#00000000"
			android:listSelector="#00000000"
			android:scrollbars="none"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:layout_weight="1.0"
			android:background="@drawable/shape_x_fff"
			android:layout_margin="1dp"
			android:id="@+id/grd"
			android:padding="1dp"/>

	</LinearLayout>

	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="match_parent"
		android:layout_height="wrap_content">

		<TextView
			android:clickable="true"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="-"
			android:gravity="center"
			android:textColor="#FF000000"
			android:textSize="16sp"
			android:padding="10dp"
			android:background="@drawable/button_x_fff"
			android:layout_margin="1dp"
			android:layout_weight="0.5"
			android:id="@+id/clear"/>

		<TextView
			android:clickable="true"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="↖"
			android:gravity="center"
			android:textColor="#FF000000"
			android:textSize="16sp"
			android:padding="10dp"
			android:background="@drawable/button_x_fff"
			android:layout_margin="1dp"
			android:layout_weight="0.5"
			android:id="@+id/copy"/>

		<TextView
			android:clickable="true"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="↘"
			android:gravity="center"
			android:textColor="#FF000000"
			android:textSize="16sp"
			android:padding="10dp"
			android:background="@drawable/button_x_fff"
			android:layout_margin="1dp"
			android:layout_weight="0.5"
			android:id="@+id/paste"/>

		<TextView
			android:clickable="true"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="×"
			android:gravity="center"
			android:textColor="#FF000000"
			android:textSize="16sp"
			android:padding="10dp"
			android:background="@drawable/button_x_fff"
			android:layout_margin="1dp"
			android:layout_weight="0.5"
			android:id="@+id/del"/>

		<TextView
			android:clickable="true"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="↑"
			android:gravity="center"
			android:textColor="#FF000000"
			android:textSize="16sp"
			android:padding="10dp"
			android:background="@drawable/button_x_fff"
			android:layout_margin="1dp"
			android:layout_weight="0.5"
			android:id="@+id/submit"/>

	</LinearLayout>

	<TextView
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:gravity="center"
		android:textSize="10sp"
		android:textColor="#FF000000"
		android:singleLine="true"
		android:id="@+id/txt"/>

	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="match_parent"
		android:layout_height="wrap_content">

		<TextView
			android:clickable="true"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:gravity="center"
			android:textColor="#FF000000"
			android:textSize="16sp"
			android:padding="10dp"
			android:background="@drawable/button_x_fff"
			android:layout_margin="1dp"
			android:text="□"
			android:id="@+id/readlinewidth"/>

		<TextView
			android:clickable="true"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:gravity="center"
			android:textColor="#FF000000"
			android:textSize="16sp"
			android:padding="10dp"
			android:background="@drawable/button_x_fff"
			android:id="@+id/change"
			android:layout_weight="1.0"
			android:layout_margin="1dp"
			android:text="↹"/>

		<TextView
			android:clickable="true"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:text="◇"
			android:gravity="center"
			android:textColor="#FF000000"
			android:textSize="16sp"
			android:padding="10dp"
			android:background="@drawable/button_o_fff"
			android:id="@+id/preview"
			android:layout_margin="1dp"/>

		<TextView
			android:clickable="true"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:text="⇔"
			android:gravity="center"
			android:textColor="#FF000000"
			android:textSize="16sp"
			android:padding="10dp"
			android:background="@drawable/button_x_fff"
			android:id="@+id/change2"
			android:layout_weight="1.0"
			android:layout_margin="1dp"/>

		<TextView
			android:clickable="true"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:text="□"
			android:gravity="center"
			android:textColor="#FF000000"
			android:textSize="16sp"
			android:padding="10dp"
			android:background="@drawable/button_x_fff"
			android:layout_margin="1dp"
			android:id="@+id/defseleall"/>

	</LinearLayout>

	<LinearLayout
		android:orientation="vertical"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:gravity="center">

		<GridView
			android:cacheColorHint="#00000000"
			android:listSelector="#00000000"
			android:scrollbars="none"
			android:layout_width="match_parent"
			android:layout_height="50dp"
			android:background="@drawable/shape_x_fff"
			android:layout_margin="1dp"
			android:id="@+id/grd2"
			android:padding="1dp"/>

		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/seeklayout">

			<SeekBar
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:id="@+id/size"
				android:progressDrawable="@drawable/seekbar_fff"
				android:thumb="@drawable/thumb_o_fff"
				android:thumbOffset="0dp"
				android:splitTrack="false"
				android:layout_marginTop="1dp"
				android:layout_marginBottom="1dp"
				android:layout_marginLeft="10dp"
				android:layout_marginRight="10dp"/>

			<TextView
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center"
				android:textSize="10sp"
				android:textColor="#FF000000"
				android:singleLine="true"
				android:id="@+id/txtsize"/>

		</RelativeLayout>

		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/touchrlt">

			<SeekBar
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:progressDrawable="@drawable/seekbar_fff"
				android:thumb="@drawable/thumb_o_fff"
				android:thumbOffset="0dp"
				android:splitTrack="false"
				android:layout_marginTop="1dp"
				android:layout_marginBottom="1dp"
				android:layout_marginLeft="10dp"
				android:layout_marginRight="10dp"
				android:id="@+id/touchseek"/>

			<TextView
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center"
				android:textSize="10sp"
				android:textColor="#FF000000"
				android:singleLine="true"
				android:id="@+id/touchtxt"/>

		</RelativeLayout>

		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/touchangle_rlt">

			<SeekBar
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:progressDrawable="@drawable/seekbar_fff"
				android:thumb="@drawable/thumb_o_fff"
				android:thumbOffset="0dp"
				android:splitTrack="false"
				android:layout_marginTop="1dp"
				android:layout_marginBottom="1dp"
				android:layout_marginLeft="10dp"
				android:layout_marginRight="10dp"
				android:id="@+id/touchangle_seek"/>

			<TextView
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center"
				android:textSize="10sp"
				android:textColor="#FF000000"
				android:singleLine="true"
				android:id="@+id/touchangle_txt"/>

		</RelativeLayout>

	</LinearLayout>

	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_weight="1.0"
		android:gravity="center"
		android:padding="1dp"
		android:background="@drawable/shape_x_fff"
		android:layout_margin="1dp"
		android:id="@+id/rltimg">

		<LinearLayout
			android:orientation="vertical"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:id="@+id/lnle">

			<SeekBar
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:id="@+id/edtsize2"
				android:progressDrawable="@drawable/seekbar_fff"
				android:thumb="@drawable/thumb_o_fff"
				android:thumbOffset="0dp"
				android:splitTrack="false"
				android:layout_marginTop="1dp"
				android:layout_marginBottom="1dp"
				android:layout_marginLeft="10dp"
				android:layout_marginRight="10dp"/>

			<EditText
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_weight="1.0"
				android:textColor="#FF000000"
				android:background="@drawable/shape_x_fff"
				android:gravity="top|left"
				android:padding="3dp"
				android:textSize="12sp"
				android:id="@+id/edt"
				android:typeface="monospace"
				android:layout_margin="1dp"/>

		</LinearLayout>

		<LinearLayout
			android:orientation="vertical"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:id="@+id/lnlim"/>

	</RelativeLayout>

	<TextView
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:gravity="center"
		android:textSize="10sp"
		android:textColor="#FF000000"
		android:singleLine="true"
		android:id="@+id/txt2"
		android:visibility="gone"/>

	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="match_parent"
		android:layout_height="wrap_content">

		<TextView
			android:clickable="true"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="←"
			android:gravity="center"
			android:textColor="#FF000000"
			android:textSize="16sp"
			android:padding="10dp"
			android:background="@drawable/button_x_fff"
			android:layout_margin="1dp"
			android:layout_weight="1.0"
			android:id="@+id/back"/>

	</LinearLayout>

</LinearLayout>

